#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;

int main()
{
    int a[3],b[3],d=0,e=0;
    for(int i=0;i<3;i++)
    {
        cin>>a[i];
    }
    for(int i=0;i<3;i++)
    {
        cin>>b[i];
    }
    for(int i=0;i<3;i++)
    {
        if(a[i]>b[i])
        {
            d++;
        }
        if(a[i]<b[i])
        {
            e++;
        }
        if(a[i]==b[i])
        {
            printf("");
        }
    }
    cout<<d<<" "<<e;
    
}
