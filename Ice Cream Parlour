#include <iostream>
using namespace std;

int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int total;
        cin>>total;
        int n,a[n];
        cin>>n;
    
        for(int i=1;i<=n;i++)
        {
            cin>>a[i];
        }
     
        for(int i=1;i<n;i++)
        {
            for(int j=i+1;j<=n;j++)
            {
                if(a[i]+a[j]==total)
                {
                    cout<<i<<" "<<j;
                    cout<<endl;
                }
            }
        }
    }
   
    return 0;
   
}
